{
  "name": "gatsby-typescript-starter",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby and typscript",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dyesseyumba/gatsby-typescript-starter"
  },
  "bugs": {
    "url": "https://github.com/dyesseyumba/gatsby-typescript-starter/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "NODE_ENV=development gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project . -t stylish src/**/*.ts{,x}",
    "lint:fix": "tslint -t stylish src/**/*.ts{,x} --fix"
  },
  "dependencies": {
    "gatsby": "^2.10.4",
    "gatsby-plugin-google-analytics": "^2.1.1",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-nprogress": "^2.1.0",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-plugin-ts-loader": "^0.1.0",
    "gatsby-source-filesystem": "^2.1.1",
    "gatsby-transformer-sharp": "^2.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "devDependencies": {
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "babel-plugin-styled-components": "^1.10.4",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.0",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.1",
    "typescript": "^3.5.2"
  }
}
